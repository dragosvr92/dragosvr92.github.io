[SubtitleSequence10]
Subtitle=((DisplaySecs=0.300000),(Text="We barely got out of there alive. ",DisplaySecs=2.400000),(Text="It was like they knew we were trying to get away.",DisplaySecs=5.700000),(Text="I was relieved, but Otis wasn't doing so good.",DisplaySecs=9.300000))

