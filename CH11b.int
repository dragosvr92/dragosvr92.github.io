[SubtitleSequence19]
Subtitle=((Text="I could tell they had got to him. ",DisplaySecs=12.300000),(Text="He looked at me and took a deep breath and asked for one last favor",DisplaySecs=17.320000),(Text="It made me sick to my stomach,",DisplaySecs=19.400000),(Text="But I had to do it before he turned into one of them ... a living dead.",DisplaySecs=24.250000))

